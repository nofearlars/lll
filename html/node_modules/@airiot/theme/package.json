{
  "name": "@airiot/theme",
  "version": "4.0.2",
  "description": "AIRIOT平台前端系统主题",
  "main": "dist/index.js",
  "iotFront": "dist/front.js",
  "iot_module": true,
  "iotOrder": 10,
  "scripts": {
    "start": "airiot start",
    "build": "node outAntdLess && airiot build",
    "upload": "airiot upload",
    "deploy": "airiot install",
    "antdLess": "node outAntdLess",
    "scanner": "i18next-scanner --config .i18n.scanner.js",
    "trans": "node .i18n.translate.js",
    "locales": "node .i18n.locales.create.js",
    "i18n1": "i18next-scanner --config .i18n.scanner.js && node .i18n.translate.js &&node .i18n.locales.create.js && airiot start"
  },
  "keywords": [
    "iot"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "4.23.2",
    "antd-theme-generator": "^1.2.11",
    "axios": "^1.3.3",
    "bezier-easing": "^2.1.0",
    "css2str": "^0.1.1",
    "i18next-scanner": "^4.1.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "react": "^18.1.0",
    "react-i18next": "^12.1.5",
    "tinycolor": "^0.0.1",
    "xadmin": "^3.0.0",
    "xadmin-i18n": "^3.0.0"
  },
  "devDependencies": {
    "@airiot/devtool": "^1.0.2",
    "babel-runtime": "^6.26.0",
    "less": "^2.7.3",
    "less-loader": "^10.2.0",
    "raw-loader": "^4.0.2"
  },
  "iotDependencies": [
    "@airiot/core",
    "@airiot/dashboard",
    "@airiot/editor",
    "@airiot/table",
    "@airiot/settings",
    "@airiot/components",
    "@airiot/device",
    "@airiot/data-api",
    "@airiot/media",
    "@airiot/datasource",
    "@airiot/echarts",
    "@airiot/video",
    "@airiot/auth",
    "@airiot/log",
    "@airiot/warning",
    "@airiot/report",
    "@airiot/flow",
    "@airiot/3d",
    "@airiot/apps",
    "@airiot/debug",
    "@airiot/gis",
    "@airiot/media",
    "@airiot/message",
    "@airiot/i18n"
  ]
}
